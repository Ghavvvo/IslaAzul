{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "APIContext": "Server=localhost;Database=Prueba;Integrated Security=True;Trusted_Connection=true;TrustServerCertificate=true;"

  },
